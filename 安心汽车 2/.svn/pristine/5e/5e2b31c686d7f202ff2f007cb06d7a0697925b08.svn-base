#import "HAViobkImageView.h"

@implementation HAViobkImageView

- (id)initWithFrame:(CGRect)frame{
    self = [super initWithFrame:frame];
    //self.image = [UIImage imageNamed:@"input_bar"];
    if (self) {
        UIImageView *lineImageView = [[UIImageView alloc] initWithFrame:CGRectMake(0, frame.size.height-0.5, frame.size.width, 0.5)];
        lineImageView.backgroundColor = [UIColor blackColor];
        [self addSubview:lineImageView];
    }
    return self;
}

@end
